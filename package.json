{
  "name": "latex-book-generator",
  "version": "2.0.0",
  "description": "AI-powered LaTeX textbook generation system with interactive outline editor",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev --workspace=frontend & npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "lint": "npm run lint --workspaces",
    "type-check": "npm run type-check --workspaces",
    "deploy": "./scripts/deploy.sh",
    "db:migrate": "wrangler d1 execute latex-book-db --file=./migrations/001_init.sql"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "latex",
    "ai",
    "textbook",
    "education",
    "cloudflare"
  ],
  "author": "AI LaTeX Book Generator Team",
  "license": "MIT"
}
